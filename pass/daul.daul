// combination of all daul IR passes, processing daul's IR into a state
// appropriate for final output to lua source code

val variables = _G.require("pass.daul.variables")
val stmtexpr = _G.require("pass.daul.stmtexpr")
val blockiife = _G.require("pass.daul.blockiife")

\ir, reporter -> {
	ir = variables(ir, reporter)
	ir = stmtexpr(ir)
	ir = blockiife(ir)
}
